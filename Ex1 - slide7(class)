// a class with 3 member data and 1 member function 
class student {     // class name is student
    // constructor
    student( ) { w = 60; h = 180; g = int(random(60,100)); }
    // member data
    float w, h, r;
    int g;
    // member function
    void phen( ){   
         r = h/w;
    }
    int grade(){
      if (g > 80) {
        println("Good!!");
      }
      if (g <=80) {
        println("Study!");
      }
      return 666;
    }
}
